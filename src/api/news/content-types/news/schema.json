{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news",
    "pluralName": "news-items",
    "displayName": "News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Location": {
      "type": "string",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Content": {
      "type": "text"
    },
    "partners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partner.partner",
      "mappedBy": "news"
    },
    "Images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
